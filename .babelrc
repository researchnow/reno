{
	"env": {
		"development": {
			"presets": [["latest", {"es2015": {"modules": false}}], "react", "stage-2"],
		  "plugins": ["transform-export-extensions", "react-hot-loader/babel"]
		},
		"components": {
			"presets": ["es2015"],
			"plugins": []
		},
		"react": {
			"presets": ["latest", "react", "stage-2"],
			"plugins": ["transform-export-extensions"]
		},
		"utils": {
			"presets": ["es2015"],
			"plugins": ["transform-export-extensions", "transform-es2015-modules-umd"]
		}
	}
}
