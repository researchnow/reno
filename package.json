{
  "name": "reno",
  "version": "1.0.0",
  "description": "The biggest little UI in the world.",
  "engines": {
    "node": ">=8"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --hot --inline",
    "start:dev": "webpack-dev-server --config ./webpack.dev.config.js  --progress --colors",
    "build:dev": "webpack --config ./webpack.dev.config.js --progress --colors",
    "build": "webpack -p --config ./webpack.prod.config.js --progress --colors",
    "dist-components": "BABEL_ENV=components babel src/components/ -d dist/components/",
    "dist-react": "BABEL_ENV=react babel src/react/ -d dist/react/",
    "clean": "rm -fr dist/",
    "dist": "npm run clean && npm run dist-components && npm run dist-react",
    "prepublish": "npm run dist"
  },
  "dependencies": {
    "custom-elements-polyfill": "github:clubajax/custom-elements-polyfill",
    "heya-io": "^1.1.5",
    "hyperhtml": "^0.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-hot-loader": "^1.3.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  }
}
