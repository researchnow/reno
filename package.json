{
  "name": "reno",
  "version": "1.0.7",
  "description": "The biggest little UI in the world.",
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "registry": "http://npm-dev.researchnow.com:4873/"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.config.js  --progress --colors --open --inline --hot",
    "build:dev": "webpack --config ./webpack.dev.config.js --progress --colors",
    "build": "webpack -p --config ./webpack.prod.config.js --progress --colors",
    "dist-components": "BABEL_ENV=components babel src/components/ -d dist/components/",
    "dist-react": "BABEL_ENV=react babel src/react/ -d dist/react/",
    "dist-utils": "BABEL_ENV=utils babel src/utils/ -d dist/utils/",
    "clean": "rm -fr dist/",
    "dist": "npm run clean && npm run dist-components && npm run dist-react && npm run dist-utils",
    "prepare": "npm run dist",
    "build-font": "rm -rf ./tmpfont && node ./scripts/svg-extract.js src/images/reno-icons.svg ./tmpfont && webfont \"./tmpfont/*.svg\" -d src/fonts/ -t scss -f reno -c reno-icon -p ../fonts/ && rm -fr ./tmpfont"
  },
  "dependencies": {
    "custom-elements-polyfill": "github:clubajax/custom-elements-polyfill#v0.8.2",
    "heya-io": "^1.1.5",
    "hyperhtml": "^0.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-hot-loader": "^1.3.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "svgson": "^2.1.0",
    "url-loader": "^0.5.9",
    "webfont": "^7.1.4",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  }
}
