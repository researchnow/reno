{
  "name": "@researchnow/reno",
  "version": "1.9.1",
  "description": "The biggest little UI in the world.",
  "publishConfig": {
    "registry": "http://npm-dev.researchnow.com:4873/"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.config.js  --progress --colors --open --inline",
    "build:dev": "webpack --config ./webpack.dev.config.js --progress --colors",
    "build:font": "rm -rf ./tmpfont && node ./scripts/svg-extract.js src/images/reno-icons.svg ./tmpfont && webfont \"./tmpfont/*.svg\" -d src/fonts/ -t scss -f reno -c reno-icon -p ../fonts/ && rm -fr ./tmpfont && cp src/images/reno-icons.svg pages/",
    "build": "webpack -p --config ./webpack.prod.config.js --progress --colors",
    "dist-components": "BABEL_ENV=components babel src/components/ -d dist/components/",
    "dist-react": "BABEL_ENV=react babel src/react/ -d dist/react/",
    "dist-utils": "BABEL_ENV=utils babel src/utils/ -d dist/utils/",
    "clean": "rm -fr dist/",
    "dist": "npm run clean && npm run dist-components && npm run dist-react && npm run dist-utils",
    "prepublishOnly": "npm run dist && npm run build"
  },
  "dependencies": {
    "@clubajax/custom-elements-polyfill": "clubajax/custom-elements-polyfill",
    "@clubajax/on": "clubajax/on",
    "heya-io": "^1.2.6",
    "hyperhtml": "^2.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "fast-sass-loader": "^1.4.5",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "svgson": "^2.1.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webfont": "^8.1.4",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
